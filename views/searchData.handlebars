
    <div class="header">
        <div class="jumbotron jumbotron-fluid">
            <div class="container">
                <h1 class="display-4">EXHIBITS</h1>
                <p class="lead">
                    Select a database to view the water parameters
                </p>

                <div class="btn-group btn-group-dp">
                    <button type="button" class="btn btn-danger dropdown-toggle" data-toggle="dropdown"
                        aria-haspopup="true" aria-expanded="false">
                        Select an exhibit to view
                    </button>
                    
                    <div class="dropdown-menu dflex" style="z-index: 1">

                        {{#each exhibitNames}}

                        <a class="dropdown-item" style="background-color: white;" href="#">
                            {{name}}
                        </a>
                        <div class="dropdown-divider">
                        </div>

                        {{/each}}

                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="card card-body col-md-10 offset-md-1">
        <div class="row">
            <div class="container-dataClass">
            <div class="col-md-10 offset-md-1">
                <div class="name-container">
                    <h1>System Parameters</h1>
                    <table class="table table-striped">
                        
                        <thead>
                            <tr>                   
                                <th>Date</th>                                              
                                <th>Temp(F)</th>                                                   
                                <th>PH</th>                                                       
                                <th>Salinity</th>                                                          
                                <th>DO</th>                                                            
                                <th>Alk</th>                                   
                                <th>Ammonia</th>                                                                                              
                                <th>Nitrate</th>                                                                                       
                                <th>Nitrite</th>                                              
                                <th>Iodine</th>                                                                                                    
                                <th>Calcium</th>                              
                            </tr>
                        </thead>
                        </div>
                        
                        <tbody>


                        </tbody>
                    </table>
                    
                </div>
            </div>
        </div>
        </div>
    </div>




    <script src="public/js/searchData.js" async defer></script>
